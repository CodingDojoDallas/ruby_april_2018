<!DOCTYPE html>
<html>
<head>
  <title>Layouts</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<div class="row">
			<div class="col-md-4">
				<%= yield :left_column %>
			</div>
			<div class="col-md-4">
				<%= yield :middle_column %>
			</div>
		
			<div class="col-md-4"> 
				<%= yield :right_column %>
			</div>
		</div>
	</div>

</body>
</html>
